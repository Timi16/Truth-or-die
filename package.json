{
  "name": "debonk-game-server",
  "version": "1.0.0",
  "description": "Debonk Game Server - Real-time trading game with Avantis price feeds",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
